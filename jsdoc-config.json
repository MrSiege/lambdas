{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": ["source/src"],
    "exclude": ["node_modules"],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "node_modules"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "template": "node_modules/docdash"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "docdash": {
    "search": true,
    "wrap": true,
    "meta": {
      "title": "gracefully",
      "description": "javascript 函数式编程库"
    }
  }
} 