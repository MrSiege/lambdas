{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": [
      "source/",
      "source/array",
      "source/collection",
      "source/data.structure",
      "source/function",
      "source/lang",
      "source/monad",
      "source/object",
      "source/utils"
    ],
    "exclude": ["node_modules"],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "node_modules"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "template": "node_modules/docdash"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "docdash": {
    "search": true,
    "wrap": true,
    "meta": {
      "title": "lambdasλ",
      "description": "纯函数式编程库"
    }
  }
} 