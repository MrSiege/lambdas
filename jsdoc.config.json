{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": [
      "src/",
      "src/array",
      "src/collection",
      "src/combinators",
      "src/data.structure",
      "src/function",
      "src/lang",
      "src/monad",
      "src/object",
      "src/utils"
    ],
    "exclude": ["node_modules"],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "node_modules"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "template": "node_modules/docdash",
    "readme": "README.md",
    "destination": "docs",
    "encoding": "utf8",
    "verbose": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "docdash": {
    "search": true,
    "wrap": true,
    "meta": {
      "title": "lambdasλ",
      "description": "纯函数式编程库"
    }
  }
} 